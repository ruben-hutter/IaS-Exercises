# example network configuration as Task2 b
# always declare all peers first using "addr" and then declare the links using "links"

# peer addresses. format: addr<peer_id>:<ip_addr>:<port>
addr1:192.168.122.9:5001
addr2:192.168.122.9:5002
addr3:192.168.122.9:5003
addr4:192.168.122.9:5004
addr5:192.168.122.9:5005
addr6:192.168.122.25:5006
addr7:192.168.122.25:5007
addr8:192.168.122.25:5008
addr9:192.168.122.25:5009

# peer links. format: link<peer_id>:<peer_id rtt,...>
link1:2 5,3 2
link2:1 5,5 2,4 5
link3:1 2,5 2
link4:2 5,7 5,8 2,6 1
link5:2 2,3 2
link6:4 1,8 1,9 2
link7:4 5,8 2
link8:4 2,7 2,6 1,9 1
link9:6 2,8 1